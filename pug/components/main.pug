main.main
  section.relevants
    article.relevants-media
      h2.relevants-media-title #{relevants1Title}
      p.relevants-media-text #{relevants1Text}
      button.relevants-media-button #{relevantsButtons[0]}
      figure.relevants-media-imgBox
        img(src=relevants1img[0], alt=relevants1img[1])
    articule.relevants-written
      figure.relevants-written-imgBox
        img(src=relevants2img[0], alt=relevants2img[1])
      div.relevants-written-texts
        span.relevants-written-texts-title1 #{relevants2Title[0]}
        span.relevants-written-texts-title2 #{relevants2Title[1]}
        span.relevants-written-texts-date #{relevants2Title[2]}
      p.relevants-written-text #{relevants2Text}
      button.relevants-written-button #{relevantsButtons[1]}
    div.relevants-publicity
      img(src=relevantsPublicityImg[0], alt=relevantsPublicityImg[1])
  section.headline
    figure.headline-imgBox
      img(src=headlineImg[0], alt=headlineImg[1])
    div.headline-texts
      h3.headline-texts-caption #{headlineCaption}
      div.headline-texts-button
        button
          img(src=headlineBtnIcon[0], alt=headlineBtnIcon[1])
    h1.headline-title #{headlineTitle}
    p.headline-text #{headlineText}
    div.headline-author
      figure.headline-author-imgBox
        img(src=headlineAuthorImg[0], alt=headlineAuthorImg[1])
      h3.headline-author-name #{headlineAuthor}
      span.headline-author-date #{headlineDate}
      button.headline-author-prev 
        img(src=headlineAuthorBtnIconPrev[0], alt=headlineAuthorBtnIconPrev[1])
      button.headline-author-next
        img(src=headlineAuthorBtnIconNext[0], alt=headlineAuthorBtnIconNext[1])
  section.mostpopular
    div
      span
      span
      button
      figure
        img(src="", alt="")
    +popularnews
    +popularnews
    +popularnews
    div
      h3
      span
      input(type="email", name="")
      button